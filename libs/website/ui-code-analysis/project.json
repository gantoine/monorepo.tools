{
  "name": "website-ui-code-analysis",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/website/ui-code-analysis/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:swc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/website/ui-code-analysis",
        "main": "libs/website/ui-code-analysis/src/index.ts",
        "tsConfig": "libs/website/ui-code-analysis/tsconfig.lib.json",
        "assets": ["libs/website/ui-code-analysis/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/website/ui-code-analysis/jest.config.ts"
      }
    }
  },
  "tags": ["scope:website", "type:ui"]
}
